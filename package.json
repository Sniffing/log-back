{
  "name": "log-back",
  "version": "0.0.1",
  "description": "back end",
  "main": "server.js",
  "dependencies": {
    "@google-cloud/datastore": "^5.1.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "csv-reader": "^1.0.6",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "moment": "^2.26.0",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/express-fileupload": "^1.1.3",
    "eslint": "^6.8.0",
    "import-sort-style-eslint": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^25.3.0",
    "jest-date-mock": "^1.0.8",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lintfix": "eslint src/**/*.ts --fix",
    "test:watch": "jest server/* --watch",
    "prettier": "prettier --check src/**/*.ts",
    "prettify": "prettier --write src/**/*.ts",
    "start": "tsc && nodemon --exec babel-node dist/server.js",
    "build": "rm -rf dist && tsc",
    "debug": "tsc && nodemon --inspect dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sniffing/log-back.git"
  },
  "author": "Terence Tse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sniffing/log-back/issues"
  },
  "homepage": "https://github.com/Sniffing/log-back#readme",
  "nodemonConfig": {
    "verbose": true,
    "ignore": [],
    "delay": "1000"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "jest": {
    "setupFiles": [
      "jest-date-mock"
    ],
    "testPathIgnorePatterns": [
      "dist"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "eslint",
      "parser": "typescript"
    }
  }
}
